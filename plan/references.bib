  shorttitle = {The \${{Gaia}}\$-\${{Kepler}}\$ {{Stellar Properties Catalog I}}},
  author = {Berger, Travis A. and Huber, Daniel and {van Saders}, Jennifer L. and Gaidos, Eric and Tayar, Jamie and Kraus, Adam L.},
  year = {2020},
